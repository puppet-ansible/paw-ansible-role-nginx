{
  "puppet_task_version": 1,
  "supports_noop": false,
  "description": "Execute the entire Ansible role imperatively",
  "input_method": "stdin",
  "parameters": {
    "nginx_user": {
      "description": "",
      "type": "Optional[String]"
    },
    "nginx_error_log": {
      "description": "Ansible role variable (default: /var/log/nginx/error.log warn)",
      "type": "Optional[String]"
    },
    "nginx_pidfile": {
      "description": "",
      "type": "Optional[String]"
    },
    "nginx_worker_processes": {
      "description": "Ansible role variable (default: \"{{ ansible_processor_vcpus | default(ansible_processor_count) }}\")",
      "type": "Optional[String]"
    },
    "nginx_extra_conf_options": {
      "description": "Ansible role variable (default: )",
      "type": "Optional[String]"
    },
    "nginx_worker_connections": {
      "description": "Ansible role variable (default: 1024)",
      "type": "Optional[String]"
    },
    "nginx_multi_accept": {
      "description": "Ansible role variable (default: off)",
      "type": "Optional[String]"
    },
    "nginx_mime_file_path": {
      "description": "",
      "type": "Optional[String]"
    },
    "nginx_server_names_hash_bucket_size": {
      "description": "Ansible role variable (default: 64)",
      "type": "Optional[String]"
    },
    "nginx_client_max_body_size": {
      "description": "Ansible role variable (default: 64m)",
      "type": "Optional[String]"
    },
    "nginx_access_log": {
      "description": "Ansible role variable (default: /var/log/nginx/access.log main buffer=16k flush=2m)",
      "type": "Optional[String]"
    },
    "nginx_sendfile": {
      "description": "Ansible role variable (default: on)",
      "type": "Optional[String]"
    },
    "nginx_tcp_nopush": {
      "description": "Ansible role variable (default: on)",
      "type": "Optional[String]"
    },
    "nginx_tcp_nodelay": {
      "description": "Ansible role variable (default: on)",
      "type": "Optional[String]"
    },
    "nginx_keepalive_timeout": {
      "description": "Ansible role variable (default: 75)",
      "type": "Optional[String]"
    },
    "nginx_keepalive_requests": {
      "description": "Ansible role variable (default: 600)",
      "type": "Optional[String]"
    },
    "nginx_server_tokens": {
      "description": "Ansible role variable (default: on)",
      "type": "Optional[String]"
    },
    "nginx_proxy_cache_path": {
      "description": "Ansible role variable (default: )",
      "type": "Optional[String]"
    },
    "server": {
      "description": "",
      "type": "Optional[String]"
    },
    "nginx_conf_path": {
      "description": "",
      "type": "Optional[String]"
    },
    "nginx_vhost_path": {
      "description": "",
      "type": "Optional[String]"
    },
    "ansible_distribution_major_version": {
      "description": "",
      "type": "Optional[String]"
    },
    "nginx_default_release": {
      "description": "Used only for Debian/Ubuntu installation, as the -t option for apt.",
      "type": "Optional[String]"
    },
    "nginx_yum_repo_enabled": {
      "description": "Used only for Redhat installation, enables source Nginx repo.",
      "type": "Optional[Boolean]"
    },
    "nginx_zypper_repo_enabled": {
      "description": "Used only for Suse installation, enables source Nginx repo.",
      "type": "Optional[Boolean]"
    },
    "nginx_ppa_use": {
      "description": "Use the official Nginx PPA for Ubuntu, and the version to use if so.",
      "type": "Optional[Boolean]"
    },
    "nginx_ppa_version": {
      "description": "Ansible role variable (default: stable)",
      "type": "Optional[String]"
    },
    "nginx_package_name": {
      "description": "The name of the nginx package to install.",
      "type": "Optional[String]"
    },
    "nginx_service_state": {
      "description": "Ansible role variable (default: started)",
      "type": "Optional[String]"
    },
    "nginx_service_enabled": {
      "description": "Ansible role variable (default: true)",
      "type": "Optional[Boolean]"
    },
    "nginx_conf_template": {
      "description": "Ansible role variable (default: nginx.conf.j2)",
      "type": "Optional[String]"
    },
    "nginx_vhost_template": {
      "description": "Ansible role variable (default: vhost.j2)",
      "type": "Optional[String]"
    },
    "nginx_extra_http_options": {
      "description": "Ansible role variable (default: )",
      "type": "Optional[String]"
    },
    "nginx_remove_default_vhost": {
      "description": "",
      "type": "Optional[Boolean]"
    },
    "nginx_listen_ipv6": {
      "description": "Listen on IPv6 (default: true)",
      "type": "Optional[Boolean]"
    },
    "nginx_vhosts": {
      "description": "Ansible role variable (default: [])",
      "type": "Optional[Array]"
    },
    "nginx_upstreams": {
      "description": "Ansible role variable (default: [])",
      "type": "Optional[Array]"
    },
    "nginx_log_format": {
      "description": "Ansible role variable (default: '$remote_addr - $remote_user [$time_local] \"$request\" '\n'$status $body_bytes_sent \"$http_referer\" '\n'\"$http_user_agent\" \"$http_x_forwarded_for\"')",
      "type": "Optional[String]"
    },
    "par_tags": {
      "description": "An array of Ansible tags to execute (optional)",
      "type": "Optional[Array[String]]"
    },
    "par_skip_tags": {
      "description": "An array of Ansible tags to skip (optional)",
      "type": "Optional[Array[String]]"
    },
    "par_start_at_task": {
      "description": "The name of the task to start execution at (optional)",
      "type": "Optional[String]"
    },
    "par_limit": {
      "description": "Limit playbook execution to specific hosts (optional)",
      "type": "Optional[String]"
    },
    "par_verbose": {
      "description": "Enable verbose output from Ansible (optional)",
      "type": "Optional[Boolean]"
    },
    "par_check_mode": {
      "description": "Run Ansible in check mode (dry-run) (optional)",
      "type": "Optional[Boolean]"
    },
    "par_timeout": {
      "description": "Timeout in seconds for playbook execution (optional)",
      "type": "Optional[Integer]"
    },
    "par_user": {
      "description": "Remote user to use for Ansible connections (optional)",
      "type": "Optional[String]"
    },
    "par_env_vars": {
      "description": "Additional environment variables for ansible-playbook execution (optional)",
      "type": "Optional[Hash]"
    }
  },
  "implementations": [
    {
      "name": "paw_ansible_role_nginx.rb",
      "requirements": [
        "puppet-agent"
      ]
    }
  ]
}